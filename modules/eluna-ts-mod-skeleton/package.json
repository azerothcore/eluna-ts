{
  "name": "eluna-ts-mod-skeleton",
  "version": "1.0.0",
  "description": "Skeleton repo to be used as a boilerplate for your Eluna-TS modules",
  "main": "src/index.ts",
  "scripts": {
    "build": "tstl",
    "dev": "tstl --watch",
    "test": "npx jest",
    "lint": "tslint -c tslint.json 'src/**/*.d.ts'",
    "lint:fix": "tslint --fix -c tslint.json 'src/**/*.d.ts'",
    "doc:gen": "npx typedoc --tsconfig tsconfig.json",
    "doc:serve": "npx http-server docs/typedoc",
    "version:patch": "npm version patch"
  },
  "author": "AzerothCore",
  "contributors": [
    "Yehonal"
  ],
  "license": "MIT",
  "dependencies": {
    "@azerothcore/eluna-ts-lib": "^1.0.13"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.3.1",
    "@types/jest": "^26.0.23",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typedoc": "^0.20.36",
    "typedoc-plugin-pages": "^1.1.0",
    "typescript-to-lua": "^0.39.3"
  }
}
